{
  "name": "greina.io",
  "alias": ["greina.io"],
  "type": "static",
  "public": false,
  "static": {
    "headers": [
      {
        "source": "**/*.@(css|js|svg)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=86400"
          }
        ]
      },
      {
        "source": "**/*.@(jpg|jpeg|gif|png)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=86400"
          }
        ]
      }
    ]
  }
}
